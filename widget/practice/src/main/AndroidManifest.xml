<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.simple.practice">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"></action>
                <category android:name="android.intent.category.LAUNCHER"></category>
            </intent-filter>
        </activity>

        <activity android:name=".PieView.PieViewActivity"></activity>
        <activity android:name=".RadarView.RadarViewActivity"></activity>
        <activity android:name=".BezierQuad.BezierQuadActivity"></activity>
        <activity android:name=".BezierCubic.BezierCubicActivity"></activity>
        <activity android:name=".BezierCircleToHeart.BezierCircleToHeartActivity"></activity>
        <activity android:name=".MagicCircle.MagicCircleActivity"></activity>
    </application>

</manifest>